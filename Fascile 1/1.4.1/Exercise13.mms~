%Fibonacci using recursive and non-recursive method. (mod 2^62)
	   PREFIX	Main     
fn	   IS		$0
n	   IS		10
	   LOC		Data_Segment
f0	   GREG		@

	   LOC		#100
Main	   SET		$1,n
	   PUSHJ	fn,FibRec
	   ADD		fn,fn,0
	   TRAP		0,Halt,0
	   PREFIX	:

	   PREFIX	FibRec
n  IS  $0;fn_1  IS  $1;t  IS  $2
FibRec	   CMPU	     	t,n,1
	   BNP		t,9F
	   SUB		t,n,1
	   SET		$3,t
	   PUSHJ	t,FibRec
	   SET		fn_1,t
	   SUB		t,n,2
	   SET		$3,t
	   PUSHJ	t,FibRec
	   ADD		$0,fn_1,t


9H	   POP		1,0
	   PREFIX	: